    "webpack": "4.44.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.0",
    "@babel/runtime-corejs3": "^7.16.0",
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
    "@mdx-js/react": "^1.6.21",
    "antd": "^3.23.4",
    "clsx": "^1.1.1",
