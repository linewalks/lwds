    "recharts": "1.8.5",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.34.2",
    "styled-components": "^4.3.2",
    "tslib": "^2.0.3"
  },
  "peerDependencies": {
    "postcss": "^8.3.11",
