{
  "name": "lwds",
  "version": "0.0.3",
  "description": "Linewalks Design System",
  "keywords": [
    "lwds",
    "linewalks design",
    "css",
    "react-component",
    "component",
    "design",
    "frontend",
    "react"
  ],
  "homepage": "",
  "bugs": {
    "url": "https://github.com/linewalks/lwds/issues",
    "email": "system@linewalks.com"
  },
  "license": "MIT",
  "contributors": [
    "Jungwoo <jungwoo@linewalks.com> (https://linewalks.com)",
    "lahee <lahee@linewalks.com> (https://linewalks.com)",
    "jh.lee <jh.lee@linewalks.com> (https://linewalks.com)",
    "leetaehee <leetaehee@linewalks.com> (https://linewalks.com)",
    "seonyeong <seonyeong@linewalks.com> (https://linewalks.com)"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/linewalks/lwds.git"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "build": "rollup -c",
    "styleguide": "styleguidist server",
    "build:docs": "styleguidist build",
    "eslint": "eslint ./src/**/*.js ./src/**/**/*.js",
    "eslint:fix": "eslint ./src/**/*.js ./src/**/**/*.js --fix",
    "clear:branch": "git branch --merged | grep -v \\* | xargs -n 1 git branch -d",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && NODE_ENV=production yarn build && yarn build:docs && git add .",
    "docstart": "docusaurus start -p 8942",
    "docbuild": "docusaurus build"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/src/components/__tests__/__snapshots__/",
      "<rootDir>/src/components/__tests__/utils.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTest.js"
    ],
    "moduleNameMapper": {
      "^@src(.*)$": "<rootDir>/src/$1",
      "^@assets(.*)$": "<rootDir>/src/assets/$1",
      "^@components(.*)$": "<rootDir>/src/components/$1",
      "^@helpers(.*)$": "<rootDir>/src/helpers/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.165",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.2",
    "autoprefixer": "9.8.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "css-loader": "4.3.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.4.3",
    "minimist": "^1.2.2",
    "node-sass": "^4.13.1",
    "postcss": "^8.3.11",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-styleguidist": "^10.6.1",
    "react-test-renderer": "^16.8.6",
    "rollup-plugin-postcss": "4.0",
    "rollup-plugin-sass-variables": "^0.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass-loader": "^10.0.4",
    "sass-variables-loader": "^0.1.3",
    "scheduler": "0.14.0",
