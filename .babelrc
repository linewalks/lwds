{
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime",
      {
        "corejs" : { "version" : 3, "proposals": true }
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    ["module-resolver", {
      "alias": {
        "@src": "./src",
        "@assets": "./src/assets",
        "@components": "./src/components",
        "@helpers": "./src/helpers"
      }
    }]
  ]
}
