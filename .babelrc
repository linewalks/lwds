{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    ["module-resolver", {
      "alias": {
        "@src": "./src",
        "@assets": "./src/assets",
        "@components": "./src/components",
        "@helpers": "./src/helpers"
      }
    }]
  ]
}
